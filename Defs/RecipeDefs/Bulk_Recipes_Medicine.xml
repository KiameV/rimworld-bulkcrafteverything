<Defs>

  <RecipeDef>
    <defName>MakeMedicineIndustrialBulk</defName>
    <label>make medicine bulk</label>
    <description>Make medicine in bulk.</description>
    <jobString>Making medicine.</jobString>
    <workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <workAmount>6500</workAmount>
    <unfinishedThingDef>UnfinishedMedicine</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>MedicineHerbal</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Neutroamine</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Cloth</li>
          </thingDefs>
        </filter>
        <count>30</count>
      </li>
    </ingredients>
    <products>
      <MedicineIndustrial>10</MedicineIndustrial>
    </products>
    <skillRequirements>
        <Crafting>6</Crafting>
        <Intellectual>4</Intellectual>
    </skillRequirements>
      <recipeUsers>
        <li>DrugLab</li>
      </recipeUsers>
      <researchPrerequisite>MedicineProduction</researchPrerequisite>
    <workSkill>Medicine</workSkill>
  </RecipeDef>

