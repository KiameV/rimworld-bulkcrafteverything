<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>BulkShell_HighExplosive</defName>
    <label>make high explosive Shells bulk</label>
    <description>Make HE shells in bulk.</description>
    <jobString>Making HE shells.</jobString>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>6000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>200</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Chemfuel</li>
					</thingDefs>
				</filter>
				<count>100</count>
			</li>
		</ingredients>
    <products>
      <Shell_HighExplosive>10</Shell_HighExplosive>
    </products>
    <workSkill>Crafting</workSkill>
      <recipeUsers>
        <li>TableMachining</li>
      </recipeUsers>
  </RecipeDef>
  
